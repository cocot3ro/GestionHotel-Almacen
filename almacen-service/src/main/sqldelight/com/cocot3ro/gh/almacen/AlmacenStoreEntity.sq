CREATE TABLE IF NOT EXISTS almacenStoreEntity (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    name TEXT NOT NULL
);

selectAll:
SELECT *
FROM almacenStoreEntity;

selectById:
SELECT *
FROM almacenStoreEntity
WHERE id = ?;

insert:
INSERT INTO almacenStoreEntity(name)
VALUES (?);

update:
UPDATE almacenStoreEntity
SET name = ?
WHERE id = ?;

delete:
DELETE FROM almacenStoreEntity
WHERE id = ?;

lastInsertRowId:
SELECT last_insert_rowid();